// An OpenFOAM dictionary of cellModels.
// The index and name must match those listed in the cellModel class.

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

hex
{
    index 3;
    numberOfPoints 8;

    faces
    6
    (
        4(0 4 7 3)   // x-min
        4(1 2 6 5)   // x-max
        4(0 1 5 4)   // y-min
        4(3 7 6 2)   // y-max
        4(0 3 2 1)   // z-min
        4(4 5 6 7)   // z-max
    );

    edges
    12
    (
        (0 1)      // x-direction
        (3 2)
        (7 6)
        (4 5)
        (0 3)      // y-direction
        (1 2)
        (5 6)
        (4 7)
        (0 4)      // z-direction
        (1 5)
        (2 6)
        (3 7)
    );
}


